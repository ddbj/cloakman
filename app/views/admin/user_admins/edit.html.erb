<%= render "admin/users/header", user: @user %>

<%= bootstrap_form_with model: @user, url: admin_user_admin_path(@user) do |form| %>
  <%= form.select :inet_user_status,    User.inet_user_status.options %>
  <%= form.select :account_type_number, User.account_type_number.options %>

  <%= form.static_control :last_sign_in_at, value: @user.last_sign_in_at.then { _1 ? l(_1) : "-" } %>
  <%= form.static_control :updated_at,      value: @user.updated_at.then { _1 ? l(_1) : "-" } %>

  <%= form.primary "Update User" %>
<% end %>
