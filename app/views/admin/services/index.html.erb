<h1 class="mb-3">Services</h1>

<div class="mb-3">
  <%= link_to "New Service", new_admin_service_path, class: "btn btn-primary" %>
</div>

<div class="list-group">
  <% @services.each do |service| %>
    <%= link_to service.username, admin_service_path(service), class: "list-group-item list-group-item-action" %>
  <% end %>
</div>
