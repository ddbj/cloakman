web: bin/rails server -p 3000
css: bin/rails dartsass:watch
keycloak: docker run --rm --network cloakman --name cloakman-keycloak --publish 8080:8080 --env KEYCLOAK_ADMIN=keycloak --env KEYCLOAK_ADMIN_PASSWORD=keycloak --volume cloakman_keycloak:/opt/keycloak/data --volume $PWD/keycloak/providers:/opt/keycloak/providers keycloak/keycloak:26.0 start-dev
openldap: docker run --rm --network cloakman --name cloakman-openldap --publish 1389:1389 --publish 1636:1636 --env LDAP_ROOT=dc=ddbj,dc=nig,dc=ac,dc=jp --env LDAP_SKIP_DEFAULT_TREE=yes --volume cloakman_openldap:/bitnami/openldap --volume $PWD/openldap/ldifs:/ldifs --volume $PWD/openldap/schemas:/schemas bitnami/openldap
mailhog: docker run --rm --network cloakman --name cloakman-mailhog --publish 1025:1025 --publish 8025:8025 mailhog/mailhog
